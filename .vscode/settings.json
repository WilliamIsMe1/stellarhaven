{
    "java.project.sourcePaths": [
        "src",
        "res"
    ],
    "java.configuration.updateBuildConfiguration": "interactive",
    "jdk.telemetry.enabled": true,
    "git.enableSmartCommit": true,
    "redhat.telemetry.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "editor.inlineSuggest.enabled": false,
    "git.confirmSync": false,
    "editor.wordWrap": "on",
    "editor.wrapOnEscapedLineFeeds": true,
    "editor.inlayHints.enabled": "off",
    "editor.fontFamily": "'Source Code Pro', 'monospace', monospace",
    "chat.agent.enabled": false,
    "chat.agent.thinkingStyle": "expanded",
    "editor.fontLigatures": false,
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "sideBar.background": "#000000",
        "sideBar.foreground": "#00ff00",
        "sideBarTitle.foreground": "#00ff00",
        "sideBarSectionHeader.foreground": "#00ff00"
    },
    "workbench.iconTheme": "vs-minimal",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "keyword.control",
                    "keyword",
                    "storage.modifier",
                    "storage.type.primitive.java",
                    "keyword.operator.new"
                ],
                "settings": {
                    "foreground": "#ff00ff",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "entity.name.type"
                ],
                "settings": {
                    "foreground": "#00ffff"
                }
            },
            {
                "scope": "variable.java",
                "settings": {
                    "foreground": "#8877ff"
                }
            },
            {
                "scope": [
                    "comment.java",
                    "comment.block",
                    "comment.block.java"
                ],
                "settings": {
                    "foreground": "#00ff00"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                    "keyword.operator.assignment",
                    "keyword.operator.arithmetic",
                    "keyword.operator.logical"
                ],
                "settings": {
                    "foreground": "#ff0000"
                }
            }
        ]
    }
}