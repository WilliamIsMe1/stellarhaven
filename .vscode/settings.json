{
    "java.project.sourcePaths": [
        "src",
        "res"
    ],
    "java.configuration.updateBuildConfiguration": "interactive",
    "jdk.telemetry.enabled": true,
    "git.enableSmartCommit": true,
    "redhat.telemetry.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "editor.inlineSuggest.enabled": false,
    "git.confirmSync": false,
    "editor.wordWrap": "on",
    "editor.wrapOnEscapedLineFeeds": true,
    "editor.inlayHints.enabled": "off",
    "editor.fontFamily": "'Source Code Pro', 'monospace', monospace",
    "chat.agent.enabled": false,
    "chat.agent.thinkingStyle": "expanded",
    "editor.fontLigatures": false,
    "workbench.colorCustomizations": {
        "editor.foreground": "#ffffff",
        "editorPane.background": "#000000",
        "editor.background": "#000000",
        "activityBar.background": "#000000",
        "activityBar.inactiveForeground": "#00ff00",
        "activityBar.foreground": "#00ff00",
        "statusBar.background": "#000000",
        "tab.activeBackground": "#000000",
        "tab.activeBorderTop": "#00ff00",
        "panel.background": "#000000"

    },
    "workbench.iconTheme": "vs-minimal"
}